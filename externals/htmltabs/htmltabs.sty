  \else
    \global\ht@init@pagegoal=\textheight\relax
    \global\advance\ht@init@pagegoal-\htInitSkip\relax
    \global\let\ht@used@footins@tolerance\@undefined
    \global\ht@currow\numexpr\ht@tempcnta+\@ne\relax
