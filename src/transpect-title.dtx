% Collect the meta information and construct the tpMaketitle macro
%    \begin{macrocode}
\def\tp@maketitle{%
  \tpNamespace{titlepage}%
  \csname tp@titlepage\endcsname
  \ifarticle
    \gdef\tpMaketitle{%
      \tpUseProperty{article-title}%
    }%
  \else
    \gdef\tpMaketitle{%
      \bgroup
        \tpUseProperty{before-titlepage}%
        \tpIfComp{Cover}{%
          \tpUseProperty{coverpage}%
        }{}%
        \null
        \tpUseProperty{before-titlepage-roman}%
        \tpUseProperty{titlepage-roman}%
        \tpUseProperty{after-titlepage}%
      \egroup
      }%
  \fi
}

%    \end{macrocode}
% Default properties for tpMaketitle
%    \begin{macrocode}
\def\tp@titlepage@default{%
  \tpSetProperty{article-title}{}%
  % Title page hooks
  \tpSetProperty{before-titlepage}{%
    \pagestyle{empty}%
    \parindent\z@
    \parskip\z@
    \raggedright
  }%
  \tpSetProperty{after-titlepage}{\pagestyle{headings}}%
  % Pages of title
  %% Cover page
  \tpSetProperty{coverpage}{%
    \bgroup
      \def\thepage{\@alph\c@page}%
      \smash{\rlap{%
          \raise\dimexpr\topsep+\headheight+\headsep+\topmargin+\topskip-\paperheight\relax
          \vtop{%
            \hskip\dimexpr-\oddsidemargin\relax
            \includegraphics[width=\paperwidth,height=\paperheight]{\tpUseComp{Cover}}%
          }}}%
      \cleardoublepage
    \egroup
  }%
  \tpSetProperty{titlepage-roman}{%
    \tpUseProperty{titlepage-i}%
    \clearpage\null
    \tpUseProperty{titlepage-ii}%
    \clearpage\null
    \tpUseProperty{titlepage-iii}%
    \clearpage\null
    \tpUseProperty{titlepage-iv}%
    \clearpage
  }%
  %% before the roman part of the title pages but after cover page
  \tpSetProperty{before-titlepage-roman}{%
    \setcounter{page}{1}%
    \def\thepage{\roman{page}}%
  }%
  \tpSetProperty{titlepage-i}{%
    \ifcollection
      \tpUseProperty{editor-string}%
    \else
      \tpUseComp{AuthorString}%
    \fi%
    \vskip\baselineskip
    \tpUseComp{Title}
  }%
  \tpSetProperty{titlepage-ii}{%
  }%
  \tpSetProperty{titlepage-iii}{%
    \ifcollection
      \tpUseProperty{editor-string}%
    \else
      \tpUseComp{AuthorString}%
    \fi%
    \par
    \tpUseComp{Title}
    \par
    \tpUseComp{Subtitle}
    \vfill
    \clearpage
  }%
  \tpSetProperty{titlepage-iv}{%
    \null\vfill
    \bgroup
      \tpUseProperty{imprint-format}%
      \tpUseComp{Biblio}%
      \par\vskip\baselineskip
      \tpUseProperty{imprint}\par%
    \egroup
    \clearpage
  }%
  %% Imprint
  \tpSetProperty{imprint-format}{\footnotesize\hsize.6\textwidth}%
  \tpSetProperty{imprint-sep}{\ifhmode\par\fi\addvspace{\baselineskip}}%
  \tpSetProperty{imprint}{%
    \tpUseProperty{publisher}%
    \tpIfComp{Qualification}{\tpUseComp{Qualification}\par}{}%
    \tpIfComp{Conversion}{\tpUseComp{Conversion}\par}{}%
    \tpIfComp{CoverDesign}{\tpUseComp{CoverDesign}\par}{}%
    \tpIfComp{CoverImage}{\tpUseComp{CoverImage}\par}{}%
    \tpIfComp{Lectorate}{\tpUseComp{Lectorate}\par}{}%
    \tpIfComp{QA}{\tpUseComp{QA}\par}{}%
    \tpIfComp{Translator}{\tpUseComp{Translator}\par}{}%
    \tpIfComp{Appraiser}{\tpUseComp{Appraiser}\par}{}%
    \tpIfComp{Discussion}{\tpUseComp{Discussion}\par}{}%
    \tpIfComp{Typesetter}{\tpUseComp{Typesetter}\par}{}%
    \tpIfComp{Print}{\tpUseComp{Print}\par}{}%
    \tpIfComp{DOI}{\tpUseComp{DOI}\par}{}%
    \tpIfComp{Keywords}{\tpUseComp{Keywords}\par}{}%
    \tpUseProperty{imprint-sep}%
    \tpUseComp{ISBN}\par%
    \tpUseComp{EpubPreText}\par%
    \tpUseComp{EISBN}\par%
    \tpUseComp{EpubISBN}\par%
    \tpUseProperty{imprint-sep}%
    \tpUseProperty{EnvDisclaimer}%
  }%
  \tpSetProperty{copyright}{%
    \tpIfComp{Copyright}
      {\tpUseComp{Copyright}}
      {\textcopyright\space\tpUseComp{Year}\space\tpUseComp{Publisher},\space\tpUseComp{PubPlace}}%
    }
  \tpSetProperty{publisher}{%
    \tpUseComp{PubDivInfo}\\%
    \tpUseProperty{copyright}\\%
    \tpUseComp{PubNote}\\%
    \tpUseComp{PubWeb}%
  }%
  % name Separators
  \tpSetProperty{editor-suffix-sgl}{\space(Hg.)}%
  \tpSetProperty{editor-suffix-pl}{\space(Hgs.)}%
  \tpSetProperty{editor-string}{%
    \tpUseComp{EditorString}%
    \ifnum\tpUseProperty{Editor-count}=\@ne
      \tpUseProperty{editor-suffix-sgl}%
    \else
      \tpUseProperty{editor-suffix-pl}%
    \fi}
  \tpSetProperty{name-and}{\space and\space}%
  \tpSetProperty{name-etal}{\space et~al.}%
  \tpSetProperty{name-sep}{,\space}%
  \tpSetProperty{corresp-mark}{*}%
}

