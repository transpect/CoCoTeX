% Shortcut for name Components
%    \begin{macrocode}
\def\tp@title@name@comp#1{%
  \tpProvideComp{tp#1String}{\expandafter\global}{}{#1String}%
  \let\tp@prev@namespace\tp@namespace
  \expandafter\newcount\csname #1-Cnt\endcsname%
  \expandafter\def\csname tp#1\endcsname{\@ifnextchar [{\csname @tp#1\endcsname}{\csname @tp#1\endcsname[]}}%]
  \expandafter\def\csname @tp#1\endcsname[##1]{%
    \def\tp@namespace{#1}%
    \expandafter\global\expandafter\advance\csname #1-Cnt\endcsname\@ne
    \if!##1!\else\expandafter\gdef\csname tp@title@#1-\expandafter\the\csname #1-Cnt\endcsname @attrs\endcsname{##1}\fi
    \tp@prov@num@comp{#1}{tpNameSimple}{simple-name}%
    \tp@prov@num@comp{#1}{tpSurName}{sur-name}%
    \tp@prov@num@comp{#1}{tpFirstName}{first-name}%
    \tp@prov@num@comp{#1}{tpMidName}{mid-name}%
    \tp@prov@num@comp{#1}{tpAffilID}{affil-id}%
    \tp@prov@num@comp{#1}{tpMail}{mail}%
    \tp@prov@num@comp{#1}{tpBio}{bio}%
  }%
  \expandafter\def\csname endtp#1\endcsname{}%
  \let\tp@namespace\tp@prev@namespace
  %%
}

