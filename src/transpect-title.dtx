% Evaluator for the Corresponding Author
%    \begin{macrocode}
\def\tp@title@eval@corresp#1#2{%
  \expandafter\ifx\csname tp@#1-#2@attr@corresp\endcsname\relax\else\tpUseProperty{corresp-mark}\fi}

%    \end{macrocode}
% This macro eventually evaluates the name family's attributes
%    \begin{macrocode}
\def\tp@eval@name@attrs#1{%
  \tp@iterate{\@tempcnta}{\@ne}{\expandafter\the\csname #1-Cnt\endcsname}{%
    \expandafter\ifx\csname tp@title@#1-\the\@tempcnta @attrs\endcsname\relax\else
      \tpParseAttributes{#1-\the\@tempcnta }{\csname tp@title@#1-\the\@tempcnta @attrs\endcsname}%
    \fi
  }}

%    \end{macrocode}
% Evaluator for the name lists
%    \begin{macrocode}
\def\tp@title@name@eval#1{%
  \tpNamespace{titlepage}%
  \expandafter\xdef\csname tp@titlepage@#1-count\endcsname{\the\csname #1-Cnt\endcsname}%
  \tp@eval@name@attrs{#1}%
  %% single Name
  \expandafter\ifnum\csname #1-Cnt\endcsname=\@ne
    \edef\@tempa{\csname tp@#1@1-simple-name\endcsname\tp@title@eval@corresp{#1}{1}}%
  \else
    %% Two names
    \expandafter\ifnum\csname #1-Cnt\endcsname=\tw@
      \edef\@tempa{\csname tp@#1@1-simple-name\endcsname\tp@title@eval@corresp{#1}{1}\tpUseProperty{name-and}\csname tp@#1@2-simple-name\endcsname\tp@title@eval@corresp{#1}{2}}%
    \else
      %% More than two names
      \def\@tempa{}%
      \tp@iterate{\@tempcnta}{\@ne}{\expandafter\the\csname #1-Cnt\endcsname}{%
        \edef\@tempb{\csname tp@#1@\the\@tempcnta-simple-name\endcsname\tp@title@eval@corresp{#1}{\the\@tempcnta}}%
        \ifnum\@tempcnta=\@ne\relax
          \let\@tempa\@tempb
        \else
          \ifnum\@tempcnta=\expandafter\the\csname #1-Cnt\endcsname\relax
            \expandafter\edef\expandafter\@tempa\expandafter{\@tempa\tpUseProperty{name-and}\@tempb}%
          \else
            \expandafter\edef\expandafter\@tempa\expandafter{\@tempa\tpUseProperty{name-sep}\@tempb}%
          \fi
        \fi
      }%
    \fi
  \fi
  \expandafter\csname tp#1String\expandafter\endcsname\expandafter{\@tempa}%
}%

%    \end{macrocode}
